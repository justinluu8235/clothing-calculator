container_commands:
  01_migrate:
    command: "/var/app/venv/*/bin/python /var/app/current/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "/var/app/venv/*/bin/python /var/app/current/manage.py collectstatic --noinput"
    leader_only: true


option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: base_project.wsgi:application